[opennode]
name=CentOS-$releasever - activesys opennode packages for $basearch
baseurl=http://opennode.activesys.org/CentOS/$releasever/opennode
enabled=0
gpgcheck=1
protect=1
priority=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ActiveSys

[opennode-test]
name=CentOS-$releasever - activesys opennode packages for $basearch
baseurl=http://opennode.activesys.org/CentOS/$releasever/opennode-test
enabled=1
gpgcheck=1
protect=1
priority=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ActiveSys

